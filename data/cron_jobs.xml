<odoo>
    <data noupdate="1">
        <record id="ir_cron_resumen_diario_whatsapp" model="ir.cron">
            <field name="name">BTR WhatsApp: Resumen Diario</field>
            <field name="model_id" ref="model_resumen_diario"/>
            <field name="state">code</field>
            <field name="code">model.enviar_resumen_diario()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="active">False</field>
        </record>

        <record id="ir_cron_resumen_semanal_whatsapp" model="ir.cron">
            <field name="name">BTR WhatsApp: Resumen Semanal</field>
            <field name="model_id" ref="model_resumen_semanal"/>
            <field name="state">code</field>
            <field name="code">model.enviar_resumen_semanal()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">weeks</field>
            <field name="numbercall">-1</field>
            <field name="active">False</field>
        </record>
    </data>
</odoo>
